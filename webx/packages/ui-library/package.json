{
  "name": "ui-library",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-library/ui-library.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --e2e",
    "test:watch": "stencil test --e2e --watch",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@stencil/angular-output-target": "^0.0.7",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/react-output-target": "^0.1.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/html": "^6.3.8",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "puppeteer": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "license": "MIT"
}
