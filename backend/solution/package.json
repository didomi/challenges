{
  "name": "consents-api",
  "version": "0.1.0",
  "private": true,
  "main": "build/app.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"node_modules/.bin/ts-node src/app.ts",
    "format": "prettier --write **/*.ts",
    "lint": "eslint ./ --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rimraf build && tsc",
    "pack": "rimraf dist && ncc build --source-map",
    "release": "npm run format && npm run lint && npm run test && npm run build && npm run pack"
  },
  "dependencies": {
    "express": "^4.17.3",
    "express-pino-logger": "^7.0.0",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "pino": "^7.9.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vercel/ncc": "^0.33.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
